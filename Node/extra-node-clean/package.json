{
  "name": "node-clean",
  "packageManager": "yarn@3.6.3",
  "scripts": {
    "dev": "tsnd -r tsconfig-paths/register --respawn --clear src/index.ts",
    "build": "rimraf ./dist && tsc",
    "start": "yarn build && node dist/index.js"
  },
  "dependencies": {
    "@prisma/client": "5.3.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "env-var": "^7.4.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.4",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.6.3",
    "prettier": "^3.0.3",
    "prisma": "^5.3.1",
    "rimraf": "^5.0.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "prisma": {
    "schema": "src/data/postgres/schema.prisma"
  }
}
